{
  "name": "Get Cookie",
  "description": "Import and export cookie of current website, manage facebook cookie session.",
  "version": "2.0.0",
  "icons": { 
    "16": "icon.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "cookies",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "action": {
    "default_title": "Get Cookie",
    "default_icon": "icon_128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3
}
