{
  "name": "Get cookies",
  "version": "1.0.0",
  "description": "Import/export fbstate.json which can be used to let bot login.",
  "permissions": [
    "cookies",
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://*.facebook.com/"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo/16.png",
      "32": "logo/32.png",
      "48": "logo/48.png",
      "64": "logo/64.png",
      "128": "logo/128.png"
    }
  },
  "icons": {
    "16": "logo/16.png",
    "32": "logo/32.png",
    "48": "logo/48.png",
    "64": "logo/64.png",
    "128": "logo/128.png"
  },
  "incognito": "split",
  "manifest_version": 3
}
